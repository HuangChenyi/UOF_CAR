<%@ Register Assembly="Ede.Uof.Utility.Component.Grid" Namespace="Ede.Uof.Utility.Component" TagPrefix="Fast" %>
   
<Fast:Grid runat="server" SelectedRowCssClass="GridItemSelected" CellPadding="0" GridLines="None"  CssClass="Grid">
   <AlternatingRowStyle  CssClass="GridItemAlternating" />
   <HeaderStyle CssClass="GridHeader"/>    
    <RowStyle CssClass="GridItem" />        
    <PagerStyle CssClass="GridPager"   />
    <EnhancePagerSettings 
     FirstAltImageUrl="~/App_Themes/Fashion/images/grid/arrow_first_alt.gif"
     FirstImageUrl="~/App_Themes/Fashion/images/grid/arrow_first.gif" 
     LastAltImage="~/App_Themes/Fashion/images/grid/arrow_last_alt.gif" 
     LastImageUrl="~/App_Themes/Fashion/images/grid/arrow_last.gif" 
     NextImageUrl="~/App_Themes/Fashion/images/grid/arrow_next.gif"
     PreviousImageUrl="~/App_Themes/Fashion/images/grid/arrow_previous.gif" 
     NextIAltImageUrl="~/App_Themes/Fashion/images/grid/arrow_next_alt.gif"  
     PreviousAltImageUrl="~/App_Themes/Fashion/images/grid/arrow_previous_alt.gif" 
     PageInfoCssClass="GridPagerPagerInfo" 
     PageNumberCssClass="GridPagerNumber" 
     PageNumberCurrentCssClass="GridPagerCurrentNumber" 
     PageRedirectCssClass="GridPagerPagerInfoRedirect"
     />     
     <ExportExcelSettings CssClass="GridExportExcel" />
</Fast:Grid>

